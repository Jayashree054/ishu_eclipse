package com.practice;



public class Test_Practice {
	
	public static int count(String s , char c)
	{
	int res =0;
	for(int i=0;i<s.length();i++){
	if(s.charAt(i)==c)
	res++;
	}
	return res;

	}
	public static void main(String[] args){

	String s="welcome";
	char c= 'e';
	System.out.println(count(s,c));
	}

	
}
